{
  "version": 2,

  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python"
    },
    {
      "src": "ui/**",
      "use": "@vercel/static"
    }
  ],

  "rewrites": [
      {
          "source": "/test",
          "destination": "/ui/test.html"
      },
      {
          "source": "/api",
          "destination": "/index"
      },
      {
          "source": "/(.*)",
          "destination": "/ui/index.html"
      }
  ]
}
