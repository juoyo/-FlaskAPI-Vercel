{
  "version": 2,

  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python"
    },
    {
      "src": "ui/**",
      "use": "@vercel/static"
    }
  ],

  "rewrites": [
      {
          "source": "/(.*)",
          "destination": "/index"
      },
      {
          "source": "/api/(.*)",
          "destination": "/api/api.py"
      }
    ]
}
