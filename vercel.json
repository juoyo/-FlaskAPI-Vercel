{
  "version": 2,
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python"
    },
    {
      "src": "ui/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/ui",
      "dest": "ui/index.html"
    },
    {
      "src": "/ui/",
      "dest": "ui/index.html"
    },
    {
      "src": "/ui/(.+)",
      "dest": "ui/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/"
    },
    {
      "src": "/",
      "dest": "ui/index.html"
    },
    {
      "src": "/about",
      "dest": "/about/"
    },
    {
      "src": "/(.+)",
      "dest": "ui/$1"
    }
  ]
}
