{
  "version": 2,


  "rewrites": [
      {
          "source": "/",
          "destination": "/template/index.html"
      },
      {
          "source": "/index",
          "destination": "/template/index.html"
      },
      {
          "source": "/login",
          "destination": "/template/login.html"
      },
      {
        "source": "/scal",
        "destination": "/template/calculator/index.html"
      },
      {
          "source": "/cal",
          "destination": "/template/calculator.html"
      },
      {
          "source": "/api/flapi(.*)",
          "destination": "/api/flapi.py"
      },
      {
          "source": "/api/calculator(.*)",
          "destination": "/api/calculator.py"
      },
      {
          "source": "/(.*)",
          "destination": "/template/404.html"
      }
  ]
}
